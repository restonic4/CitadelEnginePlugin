    <name>Citadel Engine Helper</name>
    <description>A plugin that provides custom features working with Citadel engine.</description>
    <version>0.1</version>
    <vendor>restonic4</vendor>

    <extensions defaultExtensionNs="com.intellij">
        <annotator language="JAVA" implementationClass="me.restonic4.NotRecommendedAnnotator"/>
        <lang.foldingBuilder language="JAVA" implementationClass="me.restonic4.TranslationFoldingBuilder"/>
